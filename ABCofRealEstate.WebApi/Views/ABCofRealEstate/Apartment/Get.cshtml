@model ABCofRealEstate.Services.Models.Apartments.ApartmentDetailResponse
@{
    ViewBag.Title = "Apartment";
}
<p>
    <a href="">Update</a> | <a href="">Delete</a>
</p>
<p>
    @foreach (var image in Model.FullPathsImage)
    {
        <img src="@image" height="150"/>
    }
</p>
<p>Описание: @Model.Description</p>
<p>Цена: @Model.Price</p>
<p>Тип продажи: @Model.TypeSale</p>
<p>Район: @Model.District</p>
<p>Населённый пункт: @Model.Locality</p>
<p>Улица: @Model.Street</p>
<p>Тип ремонта квартиры: @Model.ConditionHouse</p>
<p>Количество комнат: @Model.CountRooms</p>
<p>Количество балконов: @Model.CountBalcony</p>
<p>Угловая: @Model.IsCorner</p>
<p>Дата опубликование объекта: @Model.DateTimePublished</p>
<p>Идентификатор объекта: @Model.Id</p>
@if (Model.Employee != null)
{
    <hr>
    @await Html.PartialAsync("~/Views/ABCofRealEstate/Team/Get.cshtml", Model.Employee)
}